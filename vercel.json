{
    "version": 2,
    "builds": [{
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "Backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "api/index.js": {
            "runtime": "nodejs18.x",
            "maxDuration": 30
        }
    },
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}